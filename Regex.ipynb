   "source": [
    "food = \"hat mat pat  @ rat ragat rasat\"\n",
    "\n",
    "regex = re.compile(\"[rmph]*at\") #starting char is r/m/p/h + anything + at\n",
    "print(re.findall('[r]*at',food))\n",
    "food = regex.sub(\"replaced\", food) # replace with food word\n",
    "print(food)"
