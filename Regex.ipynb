     ]
    }
   ],
   "source": [
    "string = \"we need to inform him with the latest information\"\n",
    "\n",
    "for i in re.finditer(\"inform\",string):\n",
    "    loctup = i.span()\n",
    "    print(loctup)\n",
    "    print(type(loctup))"
   ]
